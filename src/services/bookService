export const getBooks = async () => {
  try {
    const res = await fetch("/data/books.json");
    if (!res.ok) {
      throw new Error("Failed to fetch books");
    }
    return await res.json();
  } catch (err) {
    console.error("Error fetching books:", err);
    return [];
  }
};

